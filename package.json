{
    "name": "byte-coder-ai-agent",
    "displayName": "Byte Coder Ai Agent",
    "description": "Your All-in-One AI Coding Companion. Smart Chat, Auto-Debugging, Code Explanation, and Multi-Agent Planning. The free alternative to Copilot.",
    "icon": "images/logo.png",
    "publisher": "uthakkan",
    "author": {
        "name": "UTHAKKAN (Ajmal U K)",
        "email": "contact.uthakkan@gmail.com",
        "url": "https://uthakkan.pythonanywhere.com"
    },
    "homepage": "https://uthakkan.pythonanywhere.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/ajmal-uk/byte-coder-ai-agent"
    },
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "AI",
        "Programming Languages",
        "Snippets",
        "Machine Learning",
        "Education"
    ],
    "keywords": [
        "ai",
        "copilot",
        "chatgpt",
        "gpt-4",
        "pair programmer",
        "code assistant",
        "debug",
        "refactor",
        "intellisense",
        "autocomplete",
        "python",
        "javascript",
        "typescript",
        "java",
        "c++",
        "planner",
        "agent",
        "free ai",
        "coding bot"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "byte-ai-sidebar",
                    "title": "Byte AI",
                    "icon": "$(hubot)"
                }
            ]
        },
        "views": {
            "byte-ai-sidebar": [
                {
                    "type": "webview",
                    "id": "byteAI.chatView",
                    "name": "Chat",
                    "icon": "images/logo.png"
                }
            ]
        },
        "commands": [
            {
                "command": "byteAI.clearChat",
                "title": "Clear Chat"
            },
            {
                "command": "byteAI.explainCode",
                "title": "Byte AI: Explain Selection"
            },
            {
                "command": "byteAI.fixCode",
                "title": "Byte AI: Fix Bugs"
            },
            {
                "command": "byteAI.generateDocs",
                "title": "Byte AI: Generate Docs"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "byteAI.explainCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.fixCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateDocs",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.85.0",
        "@types/ws": "^8.18.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "uuid": "^9.0.1",
        "ws": "^8.16.0"
    }
}