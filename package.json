    "main": "./out/extension.js",
    "activationEvents": [],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "byte-ai-sidebar",
                    "title": "Byte AI",
                    "icon": "images/sidebar.svg"
                }
            ]
        },
        "views": {
            "byte-ai-sidebar": [
                {
                    "type": "webview",
                    "id": "byteAI.chatView",
                    "name": "Chat",
                    "icon": "images/sidebar.svg"
                }
            ]
        },
        "commands": [
            {
                "command": "byteAI.clearChat",
                "title": "Clear Chat"
            },
            {
                "command": "byteAI.explainCode",
                "title": "Byte AI: Explain Selection"
            },
            {
                "command": "byteAI.fixCode",
                "title": "Byte AI: Fix Bugs"
            },
            {
                "command": "byteAI.refactorCode",
                "title": "Byte AI: Refactor Code"
            },
            {
                "command": "byteAI.generateTest",
                "title": "Byte AI: Generate Tests"
            },
            {
                "command": "byteAI.generateDocs",
                "title": "Byte AI: Generate Docs"
            },
            {
                "command": "byteAI.quickAsk",
                "title": "Byte AI: Quick Ask"
            },
            {
                "command": "byteAI.optimizeCode",
                "title": "Byte AI: Optimize Performance"
            },
            {
                "command": "byteAI.securityCheck",
                "title": "Byte AI: Security Audit"
            },
            {
                "command": "byteAI.reviewCode",
                "title": "Byte AI: Code Review"
            }
        ],
        "keybindings": [
            {
                "command": "byteAI.explainCode",
                "key": "ctrl+alt+e",
                "mac": "cmd+alt+e",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "byteAI.fixCode",
                "key": "ctrl+alt+f",
                "mac": "cmd+alt+f",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "byteAI.refactorCode",
                "key": "ctrl+alt+r",
                "mac": "cmd+alt+r",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "byteAI.quickAsk",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "editorTextFocus && editorHasSelection"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "byteAI.explainCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.fixCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.refactorCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateTest",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateDocs",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.optimizeCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.securityCheck",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.reviewCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                }
            ]
        },
        "configuration": {
            "title": "Byte Coder AI",
            "properties": {
                "byteAI.customInstructions": {
                    "type": "string",
                    "default": "",
                    "description": "Custom instructions to personalize Byte AI's behavior (e.g., 'Always use Python', 'Be concise').",
                    "editPresentation": "multiline"
                },
                "byteAI.autoContext": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically gather relevant context (open files, errors) when asking questions."
                },
                "byteAI.autoExecute": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically create files without confirmation for simple tasks (max 3 files, no commands)."
                },
                "byteAI.useLocalModel": {
                    "type": "boolean",
                    "default": false,
                    "description": "Use local Ollama model instead of Cloud API."
                },
                "byteAI.localModelName": {
                    "type": "string",
                    "default": "llama3",
                    "description": "Name of the local Ollama model to use (e.g., llama3, mistral)."
                },
                "byteAI.debugSearchAgent": {
                    "type": "boolean",
                    "default": false,
                    "description": "Include a short debug summary of which files and sections SearchAgent used."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "test": "echo \"No tests specified\""
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.85.0",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.2",
        "typescript": "^5.3.3"
    }
}
