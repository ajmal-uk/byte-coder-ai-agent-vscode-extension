{
    "name": "byte-coder-ai-agent",
    "displayName": "Byte Coder AI Agent",
    "description": "An advanced AI agent that can plan, architect, and execute code changes.",
    "version": "1.2.0",
    "publisher": "uthalkkan",
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#050608",
        "theme": "dark"
    },
    "author": {
        "name": "Ajmal U K",
        "email": "contact.uthakkan@gmail.com",
        "url": "https://uthakkan.pythonanywhere.com"
    },
    "homepage": "https://uthakkan.pythonanywhere.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/ajmal-uk/byte-coder-ai-agent"
    },
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "AI",
        "Programming Languages",
        "Machine Learning",
        "Education",
        "Snippets"
    ],
    "keywords": [
        "coding assistant",
        "productivity",
        "autonomous",
        "ai agent",
        "cursor",
        "devin",
        "cline",
        "copilot",
        "gpt-4",
        "deepseek",
        "claude",
        "pair programmer",
        "debugging",
        "automation",
        "python",
        "javascript",
        "typescript",
        "chatgpt",
        "free ai",
        "refactoring",
        "unit testing",
        "code explanation",
        "developer tools",
        "vscode extension",
        "programming",
        "software development",
        "code analysis",
        "bug fixing",
        "documentation generation"
    ],
    "main": "./out/extension.js",
    "activationEvents": [],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "byte-ai-sidebar",
                    "title": "Byte AI",
                    "icon": "images/sidebar.svg"
                }
            ]
        },
        "views": {
            "byte-ai-sidebar": [
                {
                    "type": "webview",
                    "id": "byteAI.chatView",
                    "name": "Chat",
                    "icon": "images/sidebar.svg"
                }
            ]
        },
        "commands": [
            {
                "command": "byteAI.clearChat",
                "title": "Clear Chat"
            },
            {
                "command": "byteAI.explainCode",
                "title": "Byte AI: Explain Selection"
            },
            {
                "command": "byteAI.fixCode",
                "title": "Byte AI: Fix Bugs"
            },
            {
                "command": "byteAI.refactorCode",
                "title": "Byte AI: Refactor Code"
            },
            {
                "command": "byteAI.generateTest",
                "title": "Byte AI: Generate Tests"
            },
            {
                "command": "byteAI.generateDocs",
                "title": "Byte AI: Generate Docs"
            },
            {
                "command": "byteAI.quickAsk",
                "title": "Byte AI: Quick Ask"
            },
            {
                "command": "byteAI.optimizeCode",
                "title": "Byte AI: Optimize Performance"
            },
            {
                "command": "byteAI.securityCheck",
                "title": "Byte AI: Security Audit"
            },
            {
                "command": "byteAI.reviewCode",
                "title": "Byte AI: Code Review"
            }
        ],
        "keybindings": [
            {
                "command": "byteAI.explainCode",
                "key": "ctrl+alt+e",
                "mac": "cmd+alt+e",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "byteAI.fixCode",
                "key": "ctrl+alt+f",
                "mac": "cmd+alt+f",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "byteAI.refactorCode",
                "key": "ctrl+alt+r",
                "mac": "cmd+alt+r",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "byteAI.quickAsk",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "editorTextFocus && editorHasSelection"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "byteAI.explainCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.fixCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.refactorCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateTest",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.generateDocs",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.optimizeCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.securityCheck",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                },
                {
                    "command": "byteAI.reviewCode",
                    "group": "byteAI",
                    "when": "editorHasSelection"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/uuid": "^10.0.0",
        "@types/vscode": "^1.85.0",
        "@types/ws": "^8.18.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "uuid": "^9.0.1",
        "ws": "^8.16.0"
    }
}